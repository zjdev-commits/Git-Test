<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.Dao.UserDao">
    <select id="getPassWordByUid" >
        select password
        from user
        WHERE uid = #{uid};
    </select>
</mapper>